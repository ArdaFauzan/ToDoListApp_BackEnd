{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/todo/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/todo/:id/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  